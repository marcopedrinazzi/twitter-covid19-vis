      "marks": [
        {
          "name": "bars",
          "from": {"data": "facet"},
          "type": "rect",
          "encode": {
            "enter": {
              "y": {"scale": "pos", "field": "key"},
              "height": {"scale": "pos", "band": 1},
              "x": {"scale": "xscale", "field": "value"},
              "x2": {"scale": "xscale", "value": 0},
              "fill": {"scale": "color", "field": "key"},
              "tooltip": {
           "signal": "{title: datum.Emojis, 'Type': datum.key, 'Count':datum.value}"
          }
          }
        }
        }
      ]
    }
  ]
}
